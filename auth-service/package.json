{
  "name": "auth-service",
  "version": "1.0.0",
  "description": "Microservice d'authentification avec OAuth 2.0 et OpenID Connect",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "dev": "nodemon index.js",
    "test": "jest --coverage",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "security:audit": "npm audit",
    "security:check": "npm audit --audit-level high",
    "docker:build": "docker build -t auth-service .",
    "docker:run": "docker run -p 5001:5001 --env-file .env auth-service",
    "health:check": "curl -f http://localhost:5001/health || exit 1"
  },
  "keywords": [
    "auth",
    "oauth2",
    "openid-connect",
    "microservice",
    "jwt",
    "passport",
    "security"
  ],
  "author": "In√®s GERVAIS",
  "license": "ISC",
  "type": "commonjs",
  "dependencies": {
    "axios": "^1.8.4",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^5.1.0",
    "express-session": "^1.18.1",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.16.2",
    "passport": "^0.7.0",
    "passport-facebook": "^3.0.0",
    "passport-google-oauth20": "^2.0.0"
  }
}
